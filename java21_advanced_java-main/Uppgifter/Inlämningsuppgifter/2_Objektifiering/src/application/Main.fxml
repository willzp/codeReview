<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <ScrollPane hbarPolicy="NEVER" layoutY="1.0" prefHeight="393.0" prefWidth="614.0" vbarPolicy="ALWAYS">
         <content>
            <TableView fx:id="table" prefHeight="391.0" prefWidth="602.0">
              <columns>
                <TableColumn fx:id="orderDateCol" prefWidth="75.0" text="OrderDate" />
                <TableColumn fx:id="regionCol" prefWidth="75.0" text="Region" />
                  <TableColumn fx:id="rep1Col" prefWidth="75.0" text="Rep1" />
                  <TableColumn fx:id="rep2Col" prefWidth="75.0" text="Rep2" />
                  <TableColumn fx:id="itemCol" prefWidth="75.0" text="Item" />
                  <TableColumn fx:id="unitsCol" prefWidth="75.0" text="Units" />
                  <TableColumn fx:id="unitCostCol" prefWidth="75.0" text="UnitCost" />
                  <TableColumn fx:id="totalCol" prefWidth="75.0" text="Total" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="213.0" layoutY="428.0" mnemonicParsing="false" onAction="#openCsv" prefWidth="70.0" text="CSV">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="332.0" layoutY="428.0" mnemonicParsing="false" onAction="#openJson" prefWidth="70.0" text="JSON">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
